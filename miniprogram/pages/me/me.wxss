/* index.wxss */
.scrollarea {
  height: 100vh;
}

.container {
  padding: 20px;
}

.userinfo {
  text-align: center;
  margin-bottom: 20px;
}

.userinfo-avatar {
  width: 100px;
  height: 100px;
  border-radius: 50%;
}

.userinfo-nickname {
  margin-top: 10px;
  font-size: 18px;
  font-weight: bold;
}

.user-votes {
  margin-top: 20px;
}

.user-votes text {
  display: block;
  margin-bottom: 10px;
}
